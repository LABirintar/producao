{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext", // <-- importante!
    "outDir": "dist",
    "rootDir": "src",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
